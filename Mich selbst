			local args = {...};
			if (Locking and (getnamecallmethod() == "FireServer") and (args[2] == "UpdateMousePos")) then
				args[3] = Plr.Character[getgenv().Partz].Position + (Plr.Character[getgenv().Partz].Velocity * Prediction);
				return old(unpack(args));
